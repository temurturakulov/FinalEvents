@{
    ViewBag.Title = "Запись на мероприятия";
}
<div>
    <h3>Список мероприятий</h3>
    <table>
        <tr>
            <td><p>Название мероприятия</p></td>
            <td><p>Кол-во человек</p></td>
            <td><p>Дата и время начала</p></td>
            <td><p>Дата и время окончания</p></td>
        </tr>
        @foreach(var b in ViewBag.Events)
        {
        <tr>
            <td><p>@b.EventName</p></td>
            <td><p>@b.NumberOfPeople</p></td>
            <td><p>@b.StartDate</p></td>
            <td><p>@b.EndDate</p></td>
        </tr>
        }
    </table>
</div>